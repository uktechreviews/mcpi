import mcpi.minecraft as minecraft
import mcpi.block as block
mc = minecraft.Minecraft.create()

x, y, z = mc.player.getPos()
block_type = 1
mc.setBlocks(x, y, z, x+10, y+10, z, block_type)
mc.setBlocks(x+10, y, z, x+10, y+10, z-10, block_type)
mc.setBlocks(x, y, z-10, x, y+10, z, block_type)
mc.setBlocks(x, y, z-10, x+10, y+10, z, block_type)
mc.setBlocks(x, y, z, x+10, y+10, z-10, block_type)
